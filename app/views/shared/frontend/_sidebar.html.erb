<div class="tile is-ancestor mx-1 my-2">
  <div class="tile is-vertical is-3">
    <div class="tile">
      <div class="tile is-parent">
        <article class="tile is-child notification bg-blue-100">
          <nav class="panel">
            <p class="panel-heading created-workspaces">
              我新增的工作區 
              <a id="link-add-workspace">
                <i class="fas fa-plus-square text-teal-600"></i>
              </a>
            </p>
            <div id="created-workspaces">            
            </div>
            <p class="panel-heading member-workspaces">
              我參與的工作區 
            </p>
            <div id="member-workspaces">           
          </nav>          
        </article>
      </div>
    </div>
  </div>
  <div class="tile is-parent">
    <article class="tile is-child notification bg-green-200">
        <p class="title text-md">Group</p>
        <p class="subtitle">群組</p>
        <div class="content">
          <p class="workspace-name"></p>
          <%#= yield %>
        </div>
    </article>
  </div>
</div>
<%= render 'shared/frontend/workspace_modal' %>